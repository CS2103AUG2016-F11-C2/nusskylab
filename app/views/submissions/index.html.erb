<% content_for :main_content do %>
    <div class="panel panel-info">
      <div class="panel-heading">
        <h3 class="text-center">View Team's submissions</h3>
      </div>
      <div class="panel-body">
        <%= render 'submissions_table', locals: {submissions: @submissions} %>
        <hr>
        <div class="col-sm-7 col-sm-offset-5">
          <a href="<%= new_team_submission_path(params[:team_id]) %>"
             class="btn btn-success">Submit to a milestone</a>
        </div>
      </div>
    </div>
<% end %>
