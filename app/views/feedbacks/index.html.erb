<% content_for :main_content do %>
    <% javascript 'feedbacks.js' %>
    <div class="panel panel-info">
      <div class="panel-heading">
        <h3 class="text-center">All Feedbacks</h3>
      </div>
      <div class="panel-body">
        <%= render 'feedbacks_table', locals: {feedbacks: @feedbacks} %>
        <hr>
        <div class="col-sm-7 col-sm-offset-5">
          <a href="<%= new_team_feedback_path(params[:team_id]) %>" class="btn btn-success">
            Create new feedback</a>
        </div>
      </div>
    </div>
<% end %>
