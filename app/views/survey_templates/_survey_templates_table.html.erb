<table class="table table-hover">
  <thead>
  <tr>
    <th>Milestone name</th><th>Survey Type</th><th>Actions</th>
  </tr>
  </thead>
  <tbody>
  <% locals[:survey_templates].each do |st| %>
      <tr>
        <td><%= st.milestone.name %></td>
        <td><%= st.survey_type %></td>
        <td>
          <a class="btn btn-default btn-xs" href="<%=  survey_template_path(st.id) %>">View</a>
          <a class="btn btn-default btn-xs" href="<%= survey_template_path(st.id) %>">Edit</a>
        </td>
      </tr>
  <% end %>
  </tbody>
</table>
